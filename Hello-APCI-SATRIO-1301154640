                   Program Hello; // ubah menjadi Program APCI-NIM
            Uses Crt;
            Begin
                ClrScr;
                Writeln(‘Hello’);//untuk mengeluarkan string ‘Hello’ di layar
                Writeln(‘1301154640’); //Ubah untuk menampilkan NIM Anda
                Writeln(‘anisa salama’); //Ubah untuk menampilkan Nama Anda
                Writeln(‘if 39 08’); //Ubah untuk menampilkan Kelas Anda
                Writeln(‘satrio’); //Ubah untuk menampilkan Nama Asprak Anda
                Writeln(‘1301154498’); //Ubah untuk menampilkan NIM teman anda
                Writeln(‘Modul 6’); //Ubah untuk menampilkan string ‘Modul 6
            End.
